  #main
    .rightside
      .boxheadin
        %h3 What They Can Do
      .box
        %ul.categories
          - @skills.each do |skill|
            %li
              = skill.skill
              = link_to '', skill_path(skill), :method => :delete, :class => "delete", :remote => true
      / /BOX
      = link_to_resume
      .boxhead
        %h3 Their Work
      .box
        - @works.each do |work|
          %ul.upload
            %li.file
              - if work.image_approved
                = link_to work.work_file_name, work.work.url
